{
  "name": "console",
  "private": true,
  "version": "0.0.0",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "dev": "npm run dev --workspace=@console/web",
    "dev:api": "cd packages/api && bun run dev",
    "dev:all": "cd packages/api && bun run dev & cd packages/web && npm run dev",
    "build": "npm run build --workspace=@console/web",
    "start:api": "cd packages/api && bun run start",
    "lint": "npm run lint --workspace=@console/web"
  }
}
